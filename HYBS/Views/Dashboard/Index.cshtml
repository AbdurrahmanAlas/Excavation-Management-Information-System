@*@using EntityLayer.Concrete
    @model List<>*@
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>HAFRİYAT YÖNETİM BİLGİ SİSTEMİ</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet" href="~/yenilayout/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/yenilayout/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="~/yenilayout/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/yenilayout/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/yenilayout/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/yenilayout/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/yenilayout/plugins/summernote/summernote-bs4.min.css">
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <!-- Preloader -->
        <!-- Navbar -->
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <!-- Content Wrapper. Contains page content -->
        <div>
            <!-- Content Header (Page header) -->
            <div class="content-header ">
                <div class="container-fluid ">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="col-md-12" style="color:blue;margin-left:430px;"><b>⌭ İSTATİSTİK SAYFASI</b> </h1>
                        </div><!-- /.col -->

                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->
            <!-- Main content -->
            <section class="content ">
                <div class="container-fluid">
                    <!-- Small boxes (Stat box) -->


                    <div class="row">
                        <div class="col-lg-3 col-6">
                            <!-- small box -->
                            <div class="small-box bg-warning">
                                <div class="inner">
                                    <h3>@ViewBag.v3</h3>

                                    <p>SİSTEMDEKİ FİRMA SAYISI</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-person-add"></i>
                                </div>
                                <a href="/Company/Index" class="small-box-footer">FİRMA LİSTESİ <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>
                        <div class="col-lg-3 col-6">
                            <!-- small box -->
                            <div class="small-box bg-danger">
                                <div class="inner">
                                    <h3>@ViewBag.v4</h3>

                                    <p>GPS TAKTIRAN ARAÇ ADETİ  </p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-pie-graph"></i>
                                </div>
                                <a href="/SmallVehicle/VehicleListByCompany1" class="small-box-footer">ARAÇ LİSTESİ <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>
                        <div class="col-lg-3 col-6">
                            <!-- small box -->
                            <div class="small-box bg-gradient-dark">
                                <div class="inner">
                                    <h3>@ViewBag.v5<sup style="font-size: 20px"></sup></h3>

                                    <p>SİSTEMDEKİ KÜÇÜK ARAÇ ADETİ</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="/SmallVehicle/Index" class="small-box-footer">KÜÇÜK ARAÇ LİSTESİ  <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>
                        <div class="col-lg-3 col-6">
                            <!-- small box -->
                            <div class="small-box bg-success">
                                <div class="inner">
                                    <h3>@ViewBag.v8</h3>

                                    <p>TOPLAM CEZA SAYISI  </p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-pie-graph"></i>
                                </div>
                                <a href="/Ceza/Index" class="small-box-footer">CEZA LİSTESİ <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>

                        <div class="col-lg-3 col-6">
                            <!-- small box -->
                            <div class="small-box bg-gradient-dark">
                                <div class="inner">
                                    <h3>@ViewBag.v1</h3>

                                    <p>TOPLAM YAPI RUHSATLI İŞ TALEBİ</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="/Structure/Index" class="small-box-footer">YAPI RUHSATLI İŞ TALEBİ <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>
                        <!-- ./col -->
                        <div class="col-lg-3 col-6">
                            <!-- small box -->
                            <div class="small-box bg-success">
                                <div class="inner">
                                    <h3>@ViewBag.v2<sup style="font-size: 20px"></sup></h3>

                                    <p>TOPLAM DOLGU İŞ TALEBİ</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="/Filling/Index" class="small-box-footer">İNŞAAT DOLGU İŞ TALEBİ <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>
                        <div class="col-lg-3 col-6">
                            <!-- small box -->
                            <div class="small-box bg-indigo">
                                <div class="inner">
                                    <h3>@ViewBag.v5<sup style="font-size: 20px"></sup></h3>

                                    <p>SİSTEMDEKİ KÜÇÜK ARAÇ ADETİ</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="/SmallVehicle/Index" class="small-box-footer">KÜÇÜK ARAÇ LİSTESİ  <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>

                        <div class="col-lg-3 col-6">
                            <!-- small box -->
                            <div class="small-box bg-danger">
                                <div class="inner">
                                    <h3>@ViewBag.v7<sup style="font-size: 20px"></sup></h3>

                                    <p>ALTYAPI İŞ TALEBİ</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="/Altyapı/Index" class="small-box-footer">ALTYAPI İŞ TALEBİ  <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>

                        <div class="col-lg-4 col-6">
                            <!-- small box -->
                            <div class="small-box bg-gradient-primary">
                                <div class="inner">
                                    <h3>@ViewBag.d11 ₺<sup style="font-size: 20px"></sup></h3>

                                    <p>TOPLAM BAKİYE</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="/CompanyBalance/Index" class="small-box-footer">FİRMA BAKİYELERİ  <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>
                        <div class="col-lg-4 col-6">
                            <!-- small box -->
                            <div class="small-box bg-gradient-primary">
                                <div class="inner">
                                    <h3>@ViewBag.kazıtoplam (m^3)<sup style="font-size: 20px"></sup></h3>

                                    <p> KAZI TOPLAMI YAPI RUHSAT</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="/Structure/Index" class="small-box-footer">YapıRuhsatlı KAZI Toplamı <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>
                        <div class="col-lg-4 col-6">
                            <!-- small box -->
                            <div class="small-box bg-gradient-primary">
                                <div class="inner">
                                    <h3>@ViewBag.dolgutoplam (m^3)<sup style="font-size: 20px"></sup></h3>

                                    <p> DOLGU TOPLAMI YAPI RUHSAT</p>
                                </div>
                                <div class="icon">
                                    <i class="ion ion-stats-bars"></i>
                                </div>
                                <a href="/Structure/Index" class="small-box-footer">YapıRuhsatlı DOLGU Toplamı <i class="fas fa-arrow-circle-right"></i></a>
                            </div>
                        </div>

                        <!-- ./col -->
                        <!-- ./col -->
                        <!-- ./col -->
                    </div>
                    <!-- /.row -->
                    <!-- Main row -->
                    
                    <!-- /.row (main row) -->
                </div><!-- /.container-fluid -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->


        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->

</body>
</html>


