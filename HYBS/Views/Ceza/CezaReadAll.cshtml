@model IEnumerable<EntityLayer.Concrete.Punishment>
@{
    ViewData["Title"] = "CezaReadAll";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Departments | Kiaalap - Kiaalap Admin Template</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- favicon
        ============================================ -->
    <link rel="shortcut icon" type="image/x-icon" href="img/favicon.ico">
    <!-- Google Fonts
        ============================================ -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,700,900" rel="stylesheet">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/bootstrap.min.css">
    <!-- Bootstrap CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/font-awesome.min.css">
    <!-- owl.carousel CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/owl.carousel.css">
    <link rel="stylesheet" href="~/yenilayout/css/owl.theme.css">
    <link rel="stylesheet" href="~/yenilayout/css/owl.transitions.css">
    <!-- animate CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/animate.css">
    <!-- normalize CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/normalize.css">
    <!-- meanmenu icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/meanmenu.min.css">
    <!-- main CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/main.css">
    <!-- educate icon CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/educate-custon-icon.css">
    <!-- morrisjs CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/morrisjs/morris.css">
    <!-- mCustomScrollbar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/scrollbar/jquery.mCustomScrollbar.min.css">
    <!-- metisMenu CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/metisMenu/metisMenu.min.css">
    <link rel="stylesheet" href="~/yenilayout/css/metisMenu/metisMenu-vertical.css">
    <!-- calendar CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/calendar/fullcalendar.min.css">
    <link rel="stylesheet" href="~/yenilayout/css/calendar/fullcalendar.print.min.css">
    <!-- style CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/style.css">
    <!-- responsive CSS
        ============================================ -->
    <link rel="stylesheet" href="~/yenilayout/css/responsive.css">
    <!-- modernizr JS
        ============================================ -->
    <script src="~/yenilayout/js/vendor/modernizr-2.8.3.min.js"></script>
</head>

<body>

    @foreach (var k in Model)
    {
      




        <div class="product-status mg-b-15">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="product-status-wrap drp-lst">
                            <h4>Ceza Listesi</h4>
                            <div class="add-product">
                                <a href="add-department.html">Bütün cezalar</a>
                            </div>
                            <div class="asset-inner">
                                <table>
                                    <tr>
                                        <th>Ceza ID</th>
                                        <th>Firma Adı</th>
                                        <th>Ceza tarihi</th>
                                        <th>Ceza Miktarı</th>
                                        <th>Ekip</th>
                                        <th>Plaka</th>
                                        <th>No. of Students</th>
                                        <th>Setting</th>
                                    </tr>
                                    <tbody>
                                        <tr>
                                            <td>@k.CezaId</td>

                                            <td>
                                                <button class="pd-setting">@k.CezaAmont</button>
                                            </td>

                                            <td>@k.CezaTeam</td>
                                            <td>01962067309</td>
                                            <td>1500</td>
                                            <td>
                                                <button data-toggle="tooltip" title="Edit" class="pd-setting-ed"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></button>
                                                <button data-toggle="tooltip" title="Trash" class="pd-setting-ed"><i class="fa fa-trash-o" aria-hidden="true"></i></button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="custom-pagination">
                                <nav aria-label="Page navigation example">
                                    <ul class="pagination">
                                        <li class="page-item"><a class="page-link" href="#">Previous</a></li>
                                        <li class="page-item"><a class="page-link" href="#">1</a></li>
                                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                                        <li class="page-item"><a class="page-link" href="#">3</a></li>
                                        <li class="page-item"><a class="page-link" href="#">Next</a></li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
    <div class="footer-copyright-area">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="footer-copy-right">
                        <p>Copyright © 2018. All rights reserved. Template by <a href="https://colorlib.com/wp/templates/">Colorlib</a></p>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!-- jquery
        ============================================ -->
    <script src="~/yenilayout/js/vendor/jquery-1.12.4.min.js"></script>
    <!-- bootstrap JS
        ============================================ -->
    <script src="~/yenilayout/js/bootstrap.min.js"></script>
    <!-- wow JS
        ============================================ -->
    <script src="~/yenilayout/js/wow.min.js"></script>
    <!-- price-slider JS
        ============================================ -->
    <script src="~/yenilayout/js/jquery-price-slider.js"></script>
    <!-- meanmenu JS
        ============================================ -->
    <script src="~/yenilayout/js/jquery.meanmenu.js"></script>
    <!-- owl.carousel JS
        ============================================ -->
    <script src="~/yenilayout/js/owl.carousel.min.js"></script>
    <!-- sticky JS
        ============================================ -->
    <script src="~/yenilayout/js/jquery.sticky.js"></script>
    <!-- scrollUp JS
        ============================================ -->
    <script src="~/yenilayout/js/jquery.scrollUp.min.js"></script>
    <!-- mCustomScrollbar JS
        ============================================ -->
    <script src="~/yenilayout/js/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/yenilayout/js/scrollbar/mCustomScrollbar-active.js"></script>
    <!-- metisMenu JS
        ============================================ -->
    <script src="~/yenilayout//metisMenu/metisMenu.min.js"></script>
    <script src="~/yenilayout/js/metisMenu/metisMenu-active.js"></script>
    <!-- morrisjs JS
        ============================================ -->
    <script src="~/yenilayout/js/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/yenilayout/js/sparkline/jquery.charts-sparkline.js"></script>
    <!-- calendar JS
        ============================================ -->
    <script src="~/yenilayout/js/calendar/moment.min.js"></script>
    <script src="~/yenilayout/js/calendar/fullcalendar.min.js"></script>
    <script src="~/yenilayout/js/calendar/fullcalendar-active.js"></script>
    <!-- plugins JS
        ============================================ -->
    <script src="~/yenilayout/js/plugins.js"></script>
    <!-- main JS
        ============================================ -->
    <script src="~/yenilayout/js/main.js"></script>
    <!-- tawk chat JS
        ============================================ -->
    <script src="~/yenilayout/js/tawk-chat.js"></script>
