                    <!-- /.form group -->
                    <!-- phone mask -->
                </div>

                <div class="form-group">
                    <label> Bitiş parsel:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="fas fa-phone"></i></span>
                        </div>
                        @Html.TextBoxFor(x => x.FınıshParcel, new { @class = "form-control" })
                    </div>
                    <!-- /.input group -->
                </div>
                <div class="form-group">
                    <label>Kazı Miktarı:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="fas fa-phone"></i></span>
                        </div>
                        @Html.TextBoxFor(x => x.Kazı, new { @class = "form-control" })
                    </div>
                    <!-- /.input group -->
                </div>
                <div class="form-group">
                    <label>Dolgu Miktarı:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="fas fa-phone"></i></span>
                        </div>
                        @Html.TextBoxFor(x => x.Dolgu, new { @class = "form-control" })
                    </div>
                    <!-- /.input group -->
                </div>

            </div>
            <!-- /.card-body -->
        </div>
        <!-- /.form-group -->
    </div>
    <!-- /.col -->
</div>
    <!-- /.row -->



    <button class="btn btn-info" style="margin-left:600px;"> + Firmayı Kaydet</button>
    <br />
    <br />
    <br />
}
