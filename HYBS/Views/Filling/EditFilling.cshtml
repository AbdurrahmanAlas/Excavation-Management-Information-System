    </div>
    <div class="col-12 col-sm-6">
        <div class="card card-success">
            <div class="card-header">
                <h3 class="card-title">Dolgu alınacak yer bilgileri</h3>
            </div>
            <div class="card-body">
                <!-- Date dd/mm/yyyy -->
                <!--<div class="form-group">
                    <label>İlçe:</label>

                    <div >
                        <div >
                            <span ><i class="fas fa-laptop"></i></span>
                        </div>
                       Html.DropDownListFor(x=>x.CompanyId,(List<SelectListItem>)ViewBag.cv,new { class = "form-control" })

                    </div>-->
                <!-- /.input group -->
                <!--</div>-->
                <div class="form-group">
                    <label>Başlangıç ilçe:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="far fa-list-alt"></i></span>
                        </div>
                        @Html.TextAreaFor(x => x.StartDistrict, new { @class = "form-control" })

                    </div>
                    <!-- /.input group -->
                </div>
                <!-- /.form group -->
                <!-- Date mm/dd/yyyy -->
                <div class="form-group">
                    <label>Başlangıç Ada:</label>
                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="far fa-calendar-alt"></i></span>
                        </div>
                        @Html.TextBoxFor(x => x.StartIsland, new { @class = "form-control" })
                        <!-- /.input group -->
                    </div>
                    <!-- /.form group -->
                    <!-- phone mask -->
                </div>

                <div class="form-group">
                    <label> Başlangıç parsel:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="fas fa-phone"></i></span>
                        </div>
                        @Html.TextBoxFor(x => x.StartParcel, new { @class = "form-control" })
                    </div>
                    <!-- /.input group -->
                </div>

            </div>
            <!-- /.card-body -->
        </div>
        <!-- /.form-group -->
    </div>
    <div class="col-12 col-sm-6">
        <div class="card card-success">
            <div class="card-header">
                <h3 class="card-title">Dolgu gidecek yer Bilgileri</h3>
            </div>
            <div class="card-body">
                <!-- Date dd/mm/yyyy -->
                <!--<div class="form-group">
    <label>İlçe:</label>

    <div >
        <div >
            <span ><i class="fas fa-laptop"></i></span>
        </div>
       Html.DropDownListFor(x=>x.CompanyId,(List<SelectListItem>)ViewBag.cv,new { class = "form-control" })

    </div>-->
                <!-- /.input group -->
                <!--</div>-->
                <div class="form-group">
                    <label>Bitiş ilçe:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="far fa-list-alt"></i></span>
                        </div>
                        @Html.TextAreaFor(x => x.FınıshDıstrıct, new { @class = "form-control" })

