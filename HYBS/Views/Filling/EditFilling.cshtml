@model EntityLayer.Concrete.Filling
@{
    ViewData["Title"] = "EditFilling";
    Layout = "~/Views/Shared/_Layout.cshtml";
}






@using (Html.BeginForm("EditFilling", "Filling", FormMethod.Post))
{


<div class="row">
    <div class="col-12 col-sm-12">

        <div class="card card-primary">
            <div class="card-header">
                <h3 class="card-title">Firma Bilgileri</h3>
            </div>
            <div class="card-body">
                <!-- Date dd/mm/yyyy -->
                @Html.TextBoxFor(x => x.FillingId, new { @class = "form-control" })
                <div class="form-group">
                    <label>Firma Adı:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="fas fa-laptop"></i></span>
                        </div>
                        @Html.DropDownListFor(x => x.CompanyId, (List<SelectListItem>)ViewBag.cv, new { @class = "form-control" })
                    </div>
                    <!-- /.input group -->
                </div>
                <div class="form-group">
                    <label>Talep eden:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="fas fa-laptop"></i></span>
                        </div>
                        @Html.TextBoxFor(x => x.TalepEden, new { @class = "form-control" })

                    </div>
                    <!-- /.input group -->
                </div>
                <!-- /.form group -->
                <!-- Date mm/dd/yyyy -->
                <div class="form-group">
                    <div class="input-group">
                        <label>Telefon :</label>
                        <div class="input-group">

                            <div class="input-group-prepend">
                                <span class="input-group-text"><i class="far fa-calendar-alt"></i></span>
                            </div>
                            @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })

                        </div>
                    </div>
                    <!-- /.input group -->
                </div>
                <!-- /.form group -->
                <!-- phone mask -->
                <div class="form-group">
                    <label>Tarih:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="fas fa-book"></i></span>
                        </div>
                        @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })

                    </div>
                    <!-- /.input group -->
                </div>
                <!-- /.form group -->
                <!-- phone mask -->
                <div class="form-group">
                    <label>Araç Plaka:</label>

                    <div class="input-group">
                        <div class="input-group-prepend">
                            <span class="input-group-text"><i class="fas fa-calculator"></i></span>
                        </div>
                        @Html.TextBoxFor(x => x.Vehicle, new { @class = "form-control" })

                    </div>
                    <!-- /.input group -->
                </div>
                <!-- /.form group -->
                <!-- IP mask -->
                <!-- /.form group -->

            </div>
            <!-- /.card-body -->
        </div>
    </div>
    <!-- /.col -->

